<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sisfact.sisfac.sisfact" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name="entidades.ActiveAndroid.init">


        <!-- Notice this tag which provides the database name -->
        <meta-data
            android:name="AA_DB_NAME"
            android:value="mytestdatabase.db" />

        <!-- Notice this tag which provides the database version -->
        <meta-data
            android:name="AA_DB_VERSION"
            android:value="1" />

        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MenuPrincipal"
            android:label="@string/title_activity_menu_principal"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>

        <activity
            android:name=".ver.Marca"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>

        <activity
            android:name=".guardar.Contacto"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>

        <activity
            android:name=".guardar.Marca"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>

        <activity
            android:name=".guardar.Seccciones"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>

    </application>

</manifest>
